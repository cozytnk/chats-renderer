{
    "manifest_version": 3,
    "name": "chats-renderer",
    "version": "1.0.0",
    "permissions": [
        "activeTab",
        "tabs",
        "scripting"
    ],
    "host_permissions": [
        "<all_urls>"
    ],
    "action": {
        "default_title": "action"
    },
    "background": {
        "service_worker": "service-worker.js",
        "type": "module"
    }
}